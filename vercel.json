{
  "rewrites": [
    {
      "source": "/api/agente-ia",
      "destination": "/api/agente-ia"
    },
    {
      "source": "/api/agente-ia/(.*)",
      "destination": "/api/agente-ia/$1"
    }
  ],
  "functions": {
    "api/agente-ia/index.py": {
      "memory": 1024,
      "excludeFiles": ".git/**,.vscode/**,node_modules/**,.next/**,cypress/**,docs/**,test/**,__tests__/**,coverage/**"
    }
  }
} 